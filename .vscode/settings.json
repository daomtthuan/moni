{
  // Spell checking
  "cSpell.words": ["daomtthuan", "endregion", "fontawesome", "fortawesome", "linebreak", "moni", "nativebase", "Nunito", "pexels", "tsdoc"],

  // Icons
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.opacity": 0.8,
  "material-icon-theme.saturation": 0.9,
  "material-icon-theme.files.associations": {
    // React Native
    "config": "settings",
    "settings.json": "vscode",
    "_node-version": "nodejs",
    "_ruby-version": "ruby",
    "metro.config.js": "robot",
    "app.json": "settings",
    "react-native.config.js": "react",

    // Component
    "_styles.ts": "react",
    "_hooks.ts": "svgr",
    "_configs.ts": "settings",
    "_types.ts": "typescript-def",

    // Redux
    "*.slice.ts": "redux-action",
    "*.state.ts": "redux-reducer",

    // Dotenv
    ".env.template": "template"
  },
  "material-icon-theme.folders.associations": {
    // React Native
    "_bundle": "packages",
    ".react": "react-components"
  },

  // I18n
  "i18n-ally.localesPaths": ["./src/locales"],
  "i18n-ally.keystyle": "nested",

  // Hidden files and folders
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "_bundle": false,
    "_node-version": false,
    "_ruby-version": false,
    ".buckconfig": false,
    ".editorconfig": false,
    ".flowconfig": false,
    ".gitignore": false,
    ".prettierrc.js": false,
    ".vscode": false,
    ".watchmanconfig": false,
    "Gemfile": false,
    "metro.config.js": false,
    "yarn.lock": false
  },
  "explorerExclude.backup": {}
}
